{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Music Cutter",
  "version": "1.7.0",
  "identifier": "com.ikshorin.music-cutter",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Music Cutter",
        "label": "main",
        "width": 1920,
        "height": 1080,
        "minWidth": 1200,
        "minHeight": 800,
        "decorations": true,
        "transparent": false,
        "center": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "createUpdaterArtifacts": true,
    "macOS": {
      "frameworks": []
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERBQzM4N0UxQkVFNzlBQkIKUldTN211ZSs0WWZEMnNNSGV6SG1YWEdKYmFnRDNKMHVmbyt3ZzZQM2k5ODl3REdqSXRjK0h5Q0sK",
      "endpoints": [
        "https://github.com/IlyaShorin/music-cutter/releases/latest/download/latest.json"
      ]
    }
  }
}
